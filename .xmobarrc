Config { font = "xft:Droid Sans Mono:size=15:antialias=true"
       , bgColor = "#383838"
       , fgColor = "#dfdbd2"
       , position = Top
       , lowerOnStart = False
       , commands =
         [ Run Weather "KBOS"
           [ "-t", "weather:<tempC>c"
           , "-L", "18"
           , "-H", "25"
           , "-n", "#4AC948"
           , "-h", "#DB2929"
           , "-l", "#00B7EB"] 36000
         , Run Network "wlp3s0"
           [ "-t", "<dev>:<rx>R|<tx>T"
           , "-S", "True"
           , "-L", "-1"
           , "-H", "32"
           , "-n", "#00B7EB"
           , "-h", "#DB2929"] 25
         , Run Cpu
           [ "-t", "cpu:<total>%"
           , "-m", "2"
           , "-c", "0"
           , "-a", "l"
           , "-L", "-1"
           , "-H", "50"
           , "-n", "#00B7EB"
           , "-h", "#DB2929"] 25
         , Run Memory
           [ "-t", "mem:<usedratio>%"
           , "-m", "2"
           , "-c", "0"
           , "-a", "l"
           , "-L", "-1"
           , "-H", "50"
           , "-n", "#00B7EB"
           , "-h", "#DB2929"] 25
         , Run Swap
           [ "-t", "swp:<usedratio>%"
           , "-m", "2"
           , "-c", "0"
           , "-a", "l"
           , "-L", "-1"
           , "-H", "50"
           , "-n", "#00B7EB"
           , "-h", "#DB2929"] 25
         , Run Date "%a %b %_d %Y <fc=#00B7EB>%H:%M:%S</fc>" "date" 10
         , Run StdinReader
         ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%cpu% | %memory% | %swap% | %wlp3s0% } %StdinReader% { %KBOS% | %date% "
       }
